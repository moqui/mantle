${contactInfo.postalAddress?.toName ? '<p>To: ' + contactInfo.postalAddress.toName + '</p>' : ''}
${contactInfo.postalAddress?.attnName ? '<p>Attn: ' + contactInfo.postalAddress.attnName + '</p>' : ''}
